<!DOCTYPE html>
<html>
    <head>
        <style type="text/css">
			*, *::before, *::after {
				margin: 0;
				padding: 0;
				box-sizing: border-box;
			}

            body {
                font-family: 'Poppins', 'Arial', sans-serif;
                color: #000000;
            }



			ul {
				list-style-type: none;
			}

			.header table {
				margin: 0 auto;
			}

			.main {
				margin: 0 auto;
				border-spacing: 20px;
			}

			.main > tr > td {
				padding: 20px;
				width: 30%;
				background-color: #f2f5fc;
				border-radius: 15px;
			}

			.list td:nth-child(2) {
				width: 100%;
				padding: 10px;
			}

			.list td:last-of-type span {
				padding: 5px 10px;
				background-color: #fff;
				border-radius: 5px;
				color: gray;
			}

			.list tr:not(:last-of-type) td {
				border-bottom: 1px solid #e0e0e0;
			}

			:root {
				--sprite-size: 25px;
			}

			.sprite {
				width: var(--sprite-size);
				height: var(--sprite-size);
				display: inline-block;
				background-size: 200px;
				background-image: url('data:image/png;base64,');
				background-repeat: no-repeat;
				background-position: 0 0;
				background-color: var(--background-color);
				border-radius: 5px;
			}

			/* OS */
			.sprites__Android, [class*="sprites__android_"] {
				background-position: 0 0;
			}

			.sprites__iOS, [class*="sprites__ios_"] {
				background-position: -25px 0;
			}

			.sprites__MacOS {
				background-position: -50px 0;
			}

			.sprites__Linux {
				background-position: -75px 0;
			}

			.sprites__Windows {
				background-position: -100px 0;
			}

			/* Flags */
			.sprites__CR {
				background-position: 0 -50px;
			}

			.sprites__PA {
				background-position: -25px -50px;
			}

			.sprites__GT {
				background-position: -50px -50px;
			}

			.sprites__HN {
				background-position: -75px -50px;
			}

			.sprites__NI {
				background-position: -100px -50px;
			}

			.sprites__SV {
				background-position: -125px -50px;
			}

			.sprites__MX {
				background-position: -150px -50px;
			}

			.sprites__US {
				background-position: -175px -50px;
			}

			/* Software */
			[class*="sprites__chrome_"] {
				background-position: 0 -25px;
			}

			[class*="sprites__safari_"] {
				background-position: -25px -25px;
			}

			[class*="sprites__edge_"] {
				background-position: -50px -25px;
			}

			[class*="sprites__firefox_"] {
				background-position: -75px -25px;
			}

			[class*="sprites__dart_"] {
				background-position: -100px -25px;
			}
        </style>
    </head>
    <body>
		<table class="header" style="margin: auto;">
			<tr>
				<td>
					<img width="45" height="45" src='data:image/avif;base64,'/>
				</td>
				<td>
					<h1>SkyAnalytics</h1>
				</td>
			</tr>
		</table>
		<table class="main">
			<tr>
				<td>
					<h2>Visitors</h2>
					<table class="list">
						{{#each summary}}
							<tr>
								<td><div class="sprite sprites__CR"></div></td>
								<td>{{this.name}}</td>
								<td><span>0</span></td>
							</tr>
						{{/each}}
					</table>
				</td>

				<td>
					<h2>Countries</h2>
					<table class="list">
						{{#each summary}}
							<tr>
								<td><div class="sprite"></div></td>
								<td>{{this.name}}</td>
								<td><span>0</span></td>
							</tr>
						{{/each}}
					</table>
				</td>
			</tr>
			<tr>
				<td>
					<h2>Operating Systems</h2>
					<table class="list">
						{{#each summary}}
							<tr>
								<td><div class="sprite sprites__edge_130.2.2"></div></td>
								<td>{{this.name}}</td>
								<td><span>0</span></td>
							</tr>
						{{/each}}
					</table>
				</td>

				<td>
					<h2>Software</h2>
					<table class="list">
						{{#each summary}}
							<tr>
								<td><div class="sprite"></div></td>
								<td>{{this.name}}</td>
								<td><span>0</span></td>
							</tr>
						{{/each}}
					</table>
				</td>
			</tr>
		</table>
    </body>
</html>